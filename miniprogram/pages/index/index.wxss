/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.container {
  width: 100%;
}

.map_container {
  width: 100%;
}

.cu-modal1 {
  /* position: fixed; */
  position: absolute;
  background: none;
  top: auto;
  z-index: 1110;
}


.padding-xl { 
  /* margin: 20rpx 0; */
  padding: 30rpx 10rpx;
}

.cu-bar .search-form {
  background-color: #f5f5f5;
  width: 80%;
	line-height: 100rpx;
	height: 100rpx;
	font-size: 32rpx;
	color: var(--black);
	margin: 0 10rpx;
}

.action {
  width: 20%;
}

.cu-btn {
	padding: 0 10rpx;
	font-size: 32rpx;
  height: 100rpx;
}

.cu-list.grid>.cu-item {
	padding: 30rpx 0 40rpx;
}

.cu-list.grid>.cu-item [class*=cuIcon] {
	font-size: 60rpx
}

.active {
  color: #39b54a!important;
}